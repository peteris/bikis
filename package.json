  "repository": {
    "type": "git",
    "url": "git://github.com/peteris/self.git"
  },
  "scripts": {
    "dev": "node server.js",
    "lint": "standard \"src/**/*.js\"",
    "test": "npm run lint && NODE_ENV=test jest",
    "test:watch": "npm test -- --watch",
    "start": "concurrently --kill-others \"npm run watch\" \"npm run serve-dev\"",
    "watch": "cross-env NODE_ENV=development NODE_PATH=\"./src\" UV_THREADPOOL_SIZE=100 node webpack/webpack-dev-server.js",
    "build": "cross-env NODE_ENV=production webpack --config webpack/webpack.prod.babel.js --color",
    "serve-dev": "cross-env NODE_ENV=development node ./bin/server.js",
    "serve": "cross-env NODE_ENV=production node ./bin/server.js"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/jest-css-modules",
    "moduleNameMapper": {
      "^[./a-zA-Z0-9$_-]+\\.(gif|png|jpg|jpeg)$": "<rootDir>/tests/image.stub.txt"
    },
    "unmockedModulePathPatterns": [
      "node_modules/react/",
      "node_modules/enzyme/",
      "react",
      "react-dom",
      "react-addons-test-utils",
      "fbjs",
      "enzyme",
      "cheerio",
      "htmlparser2",
      "underscore",
      "lodash",
      "domhandler",
      "object.assign",
      "define-properties",
      "function-bind",
      "object-keys",
      "object.assign",
      "core-js",
      "classnames",
      "ramda"
    ]
  },
  "standard": {
    "globals": [
      "jest",
      "describe",
      "it",
      "afterEach",
      "expect",
      "webpackIsomorphicTools",
      "__DEVELOPMENT__",
      "__DISABLE_SSR__"
    ]
  },
