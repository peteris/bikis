{
  "version": 2,
  "name": "peteris",
  "builds": [
    { "src": "static/**", "use": "@now/static" },
    { "src": "next.config.js", "use": "@now/next" },
    { "src": "api/*.js", "use": "@now/node" }
  ],
  "routes": [
    { "src": "/favicon.ico", "dest": "/static/favicon.ico" },
    { "src": "/api/(.*)", "dest": "/api/$1.js" },
    { "src": "/(.*)", "dest": "/$1" }
  ]
}
